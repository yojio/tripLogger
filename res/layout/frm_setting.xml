                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:textColor="#ffffff"
                    android:textSize="18dip"
                    android:gravity="left|center_vertical"
                    android:text="@string/setcap_auto_location_type"
                    android:layout_weight="1"
                ></TextView>
                <TextView
                    android:id="@+id/set_auto_location_type_data"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right|center_vertical"
                    android:background="#00000000"
                    android:textColor="#ffC700"
                    android:textSize="18dip"
                    android:paddingTop="5dip"
                    android:paddingRight="10dip"
                    android:gravity="right|center_vertical"
                ></TextView>
            </LinearLayout>
            <LinearLayout
                android:background="#ff666666"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:orientation="horizontal"
                android:gravity="left|center_vertical"
                android:minHeight="10dip"
                android:padding="0dip">
            </LinearLayout>
            <TextView
                android:id="@+id/set_twitter"
                android:background="@android:drawable/list_selector_background"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:orientation="horizontal"
                android:gravity="left|center_vertical"
                android:text="@string/setcap_twitter"
                android:textColor="#ffffff"
                android:textSize="18dip"
                android:minHeight="35dip"
                android:padding="0dip">
            ></TextView>
            <LinearLayout
                android:id="@+id/set_twitter_group"
                android:background="#00000000"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left"
                android:orientation="vertical"
                android:gravity="left"
                android:padding="5dip"
                android:minHeight="40dip">
                <LinearLayout
