<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/settingframe"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:gravity="fill_horizontal"
    android:background="#ff000000"
    >

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:gravity="fill_horizontal"
            android:background="#ff000000">
            <LinearLayout
                android:background="#ff666666"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:orientation="horizontal"
                android:gravity="left|center_vertical"
                android:minHeight="10dip"
                android:padding="0dip">
            </LinearLayout>
            <LinearLayout
                android:id="@+id/set_location_type"
                android:background="@android:drawable/list_selector_background"
                android:layout_width="fill_parent"
                android:layout_height="35dip"
                android:layout_gravity="left|center_vertical"
                android:orientation="horizontal"
                android:gravity="left|center_vertical"
                android:minHeight="35dip"
                android:padding="0dip">
                <TextView
                    android:background="#00000000"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:textColor="#ffffff"
                    android:textSize="18dip"
                    android:gravity="left|center_vertical"
                    android:text="@string/setcap_location_type"
                    android:layout_weight="1"
                ></TextView>
                <TextView
                    android:id="@+id/set_location_type_data"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right|center_vertical"
                    android:background="#00000000"
                    android:textColor="#ffC700"
                    android:textSize="18dip"
                    android:paddingTop="5dip"
                    android:paddingRight="10dip"
                    android:gravity="right|center_vertical"
                ></TextView>
            </LinearLayout>
            <TextView
                android:id="@+id/set_location_type_memo"
                android:background="#00000000"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:textColor="#ffffff"
                android:textSize="10dip"
                android:gravity="left|center_vertical"
                android:text="@string/setcap_location_type_memo_fine"
            ></TextView>
            <TextView
                android:id="@+id/set_location_type_memo2"
                android:background="#00000000"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:textColor="#ffffff"
                android:textSize="10dip"
                android:gravity="left|center_vertical"
                android:text="@string/setcap_location_type_memo"
            ></TextView>
            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:scaleType="centerCrop"
                android:paddingTop="5dip"
                android:paddingBottom="5dip"
                android:src="@android:drawable/divider_horizontal_dark"
            ></ImageView>
            <LinearLayout
                android:id="@+id/set_location_accuracy"
                android:background="@android:drawable/list_selector_background"
                android:layout_width="fill_parent"
                android:layout_height="35dip"
                android:layout_gravity="left|center_vertical"
                android:orientation="horizontal"
                android:gravity="left|center_vertical"
                android:minHeight="35dip"
                android:padding="0dip">
                <TextView
                    android:background="#00000000"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center_vertical"
                    android:textColor="#ffffff"
                    android:textSize="18dip"
                    android:gravity="left|center_vertical"
                    android:text="@string/setcap_location_accuracy"
                    android:layout_weight="1"
                ></TextView>
                <TextView
                    android:id="@+id/set_location_accuracy_data"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right|center_vertical"
                    android:background="#00000000"
                    android:textColor="#ffC700"
                    android:textSize="18dip"
                    android:paddingTop="5dip"
                    android:paddingRight="10dip"
                    android:gravity="right|center_vertical"
                ></TextView>
            </LinearLayout>
            <TextView
                android:id="@+id/set_location_accuracy_memo"
                android:background="#00000000"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left|center_vertical"
                android:textColor="#ffffff"
                android:textSize="10dip"
                android:gravity="left|center_vertical"
                android:text="@string/setcap_location_accuracy_memo"
            ></TextView>
            <ImageView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:scaleType="centerCrop"
                android:paddingTop="5dip"
                android:paddingBottom="5dip"
                android:src="@android:drawable/divider_horizontal_dark"
            ></ImageView>
            <LinearLayout
