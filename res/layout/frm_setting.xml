                        android:layout_height="35dip"
                        android:layout_gravity="left|center_vertical"
                        android:orientation="horizontal"
                        android:gravity="left|center_vertical"
                        android:minHeight="35dip"
                        android:padding="0dip">
                        <TextView
                            android:background="#00000000"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center_vertical"
                            android:textColor="#ffffff"
                            android:textSize="18dip"
                            android:gravity="left|center_vertical"
                            android:text="@string/setcap_upload_pic"
                            android:layout_weight="1"
                        ></TextView>
                        <TextView
                            android:id="@+id/set_twitter_upload_pic_data"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right|center_vertical"
                            android:background="#00000000"
                            android:textColor="#ffC700"
                            android:textSize="18dip"
                            android:gravity="right|center_vertical"
                            android:paddingRight="5dip"
                        ></TextView>
                    </LinearLayout>
                    <LinearLayout
                        android:id="@+id/set_twitter_upload_pic_size_area"
                        android:background="@android:drawable/list_selector_background"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:orientation="vertical"
                        android:gravity="left"
                        android:paddingLeft="20dip"
                        android:paddingRight="0dip">
                        <ImageView
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:scaleType="centerCrop"
                            android:paddingTop="5dip"
                            android:paddingBottom="5dip"
                            android:src="@android:drawable/divider_horizontal_dark"
                        ></ImageView>
                        <LinearLayout
                            android:id="@+id/set_twitter_upload_pic_type"
                            android:background="@android:drawable/list_selector_background"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|center_vertical"
                            android:orientation="horizontal"
                            android:gravity="left|center_vertical"
                            android:minHeight="35dip"
                            android:paddingLeft="0dip">
                            <TextView
                                android:background="#00000000"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left|center_vertical"
                                android:textColor="#ffffff"
                                android:textSize="18dip"
                                android:gravity="left|center_vertical"
                                android:text="@string/setcap_upload_pic_type"
                                android:layout_weight="1"
                            ></TextView>
                            <TextView
                                android:id="@+id/set_twitter_upload_pic_type_data"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="right|center_vertical"
                                android:background="#00000000"
                                android:textColor="#ffC700"
                                android:textSize="18dip"
                                android:gravity="right|center_vertical"
                                android:paddingRight="5dip"
                            ></TextView>
                        </LinearLayout>
                        <LinearLayout
                            android:id="@+id/set_twitter_twitter_id_area"
                            android:background="@android:drawable/list_selector_background"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left"
                            android:orientation="vertical"
                            android:gravity="left"
                            android:paddingLeft="20dip"
                            android:paddingRight="0dip">
                            <LinearLayout
                                android:id="@+id/set_twitter_twitter_id"
                                android:background="@android:drawable/list_selector_background"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="left|center_vertical"
                                android:orientation="horizontal"
                                android:gravity="left|center_vertical"
                                android:minHeight="35dip"
                                android:paddingLeft="0dip">
                                <TextView
